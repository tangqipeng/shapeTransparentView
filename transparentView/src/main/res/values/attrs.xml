<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShapeImageView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
        <attr name="borderLine" format="enum">
            <enum name="full" value="0"/>
            <enum name="dotted" value="1"/>
        </attr>
        <attr name="borderDotted" format="dimension"/>
        <attr name="borderBlack" format="dimension"/>
        <attr name="frameColor" format="color"/>
        <attr name="shapeView" format="enum">
            <enum name="circle" value="0"/>
            <enum name="round" value="1"/>
            <enum name="oval" value="2"/>
            <enum name="rightAngleCircle" value="3"/>
        </attr>
        <attr name="shapeRadius" format="dimension"/>
        <attr name="shapeHeight" format="dimension"/>
        <attr name="rightAngleLocation">
            <flag name="leftTop" value="0x03"/>
            <flag name="leftBottom" value="0x30"/>
            <flag name="rightTop" value="0x05"/>
            <flag name="rightBottom" value="0x50"/>
        </attr>
        <attr name="cornersX" format="dimension"/>
        <attr name="cornersY" format="dimension"/>
    </declare-styleable>

</resources>